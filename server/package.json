{
	"name": "prioritea",
	"version": "1.0.0",
	"description": "MERN CRUD",
	"main": "dist/src/server.js",
	"scripts": {
		"lint": "eslint --fix ./src/**/*.ts",
		"format": "prettier --write ./src/**/* --ignore-unknown",
		"build": "tsc",
		"watch": "tsc -w",
		"dev": "cross-env NODE_ENV=development nodemon dist/src/server.js",
		"test": "node dist/src/server.js",
		"typeorm": "typeorm-ts-node-commonjs",
		"typeorm:seeding": "ts-node ./node_modules/typeorm-seeding/dist/cli.js",
		"seed:config": "yarn typeorm:seeding config",
		"seed:run": "yarn typeorm:seeding seed",
		"schema:drop": "yarn typeorm schema:drop",
		"schema:sync": "yarn typeorm schema:sync",
		"migration:create": "yarn typeorm migration:create",
		"migration:run": "yarn typeorm migration:run",
		"migration:revert": "yarn typeorm migration:revert"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@faker-js/faker": "^6.1.2",
		"@overnightjs/core": "^1.7.6",
		"argon2": "^0.28.5",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"express-async-errors": "^3.1.1",
		"helmet": "^5.0.2",
		"http-status-codes": "^2.2.0",
		"jsonwebtoken": "^8.5.1",
		"morgan": "^1.10.0",
		"node": "^17.7.2",
		"pg": "^8.7.3",
		"reflect-metadata": "^0.1.13",
		"tslib": "^2.3.1",
		"typedi": "^0.10.0",
		"typeorm": "^0.3.6",
		"typeorm-seeding": "^1.6.1",
		"typeorm-typedi-extensions": "^0.4.1",
		"winston": "^3.7.2"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/morgan": "^1.9.3",
		"@types/node": "^17.0.25",
		"@typescript-eslint/eslint-plugin": "^5.20.0",
		"@typescript-eslint/parser": "^5.20.0",
		"editorconfig": "^0.15.3",
		"eslint": "^8.13.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"nodemon": "^2.0.15",
		"prettier": "^2.6.2",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3"
	}
}
